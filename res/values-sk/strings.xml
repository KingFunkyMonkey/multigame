    <!-- Minigames -->

    <string name="minigames_VBird"><![CDATA[Pohybuj štvorčekom <strong><u>nakláňaním</u></strong> telefónu <strong><u>dopredu</u></strong> a <strong><u>dozadu</strong></u>. Musíš sa vyhnúť prekážkam.]]></string>
    <string name="minigames_HBalance"><![CDATA[<strong><u>Nakláňaj</u></strong> mobilom <strong><u>doľava a doprava</strong></u>. Snaž sa, aby gulička nespadla na zem. Teraz musíš ovládať <strong><u>obe minihry naraz!</strong></u>]]></string>
    <string name="minigames_TCatcher"><![CDATA[Padajúce guličky nesmú spadnúť na zem. Zmeň chytajúcu guličku <strong><u>kliknutím</strong></u> na ňu. Použi svoj <strong><u>ľavý palec</strong></u> alebo ľavý ukazovák.]]></string>
    <string name="minigames_TGatherer"><![CDATA[Zbieraj guličky <strong><u>kliknutím</strong></u> na ne predtým než ubehne čas. Použi svoj <strong><u>pravý palec</strong></u> alebo ukazovák.]]></string>
    <string name="minigames_TInvader"><![CDATA[Pohybuj laserovým poľom kliknutím na obrazovku. Nepriatelia sa nesmú dostať do stredu.]]></string>
    <string name="minigames_VBouncer"><![CDATA[Pohybuj platformou nakláňaním mobilu hore a dole. Loptička sa nesmie dotknúť ľavého kraja obrazovky.]]></string>

