<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Nadpis" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#00FF00</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="Multigame_style_game" parent="@android:style/Theme.Holo.Light">
        <!--<item name="sdlDialogStyle">@style/DialogStyleLight.Custom</item>-->
        <item name="android:soundEffectsEnabled">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Multigame_style_other" parent="@android:style/Theme.Holo.Light">
        <!--<item name="sdlDialogStyle">@style/DialogStyleLight.Custom</item>-->
        <item name="android:soundEffectsEnabled">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

</resources>